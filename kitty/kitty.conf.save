# BEGIN_KITTY_FONTS
font_family      "Fira Code Regular Nerd Font Complete"
#bold_font        auto
#italic_font      auto
#bold_italic_font auto
font_size 12
enable_ligatures always
# END_KITTY_FONTS


# BEGIN_KITTY_THEME
include current-theme.conf
# END_KITTY_THEME


# BEGIN_NO_CONFIRM
confirm_os_window_close 0
# END_NO_CONFIRM

# BEGIN SHELL
shell zsh
# END SHELL
